{
	"$schema": "https://biomejs.dev/schemas/2.1.1/schema.json",
	"json": {
		"formatter": {
			"enabled": true
		},
		"linter": {
			"enabled": true
		}
	},
	"files": {
		"includes": ["src", "test"]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"complexity": {
				"noBannedTypes": "warn"
			}
		}
	}
}
