{
	"name": "@shopware-ag/app-server-sdk-dynamodb",
	"version": "1.0.0",
	"description": "DynamoDB Integration for the Shopware App Server SDK",
	"type": "module",
	"license": "MIT",
	"tshy": {
		"exports": {
			"./package.json": "./package.json",
			".": "./src/mod.ts"
		}
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/esm/mod.d.ts",
				"default": "./dist/esm/mod.js"
			},
			"require": {
				"types": "./dist/commonjs/mod.d.ts",
				"default": "./dist/commonjs/mod.js"
			}
		}
	},
	"main": "./dist/commonjs/mod.js",
	"types": "./dist/commonjs/mod.d.ts",
	"module": "./dist/esm/mod.js",
	"files": ["dist"],
	"dependencies": {
		"@shopware-ag/app-server-sdk": "workspace:^",
		"@aws-sdk/client-dynamodb": "^3.637.0",
		"@aws-sdk/lib-dynamodb": "^3.637.0"
	}
}
